<Application x:Class="SerialPower.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:SerialPower"
			 DispatcherUnhandledException="Application_DispatcherUnhandledException"
			 Startup="Application_Startup"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/SerialPower;Component/DictionaryGraphics.xaml"/>
				<ResourceDictionary Source="/SerialPower;Component/DictionaryElements.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
