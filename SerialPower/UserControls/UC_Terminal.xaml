<UserControl x:Class="SerialPower.UserControls.UC_Terminal"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:SerialPower.UserControls"
			 mc:Ignorable="d"
			 Background="{StaticResource BackgroundStyle}"
			 d:DesignHeight="720" d:DesignWidth="1280">
	<DockPanel LastChildFill="True">
		<Label DockPanel.Dock="Top" Style="{StaticResource TitleStyle}">Terminal</Label>
		<TextBox x:Name="TextBoxCommandInput" DockPanel.Dock="Bottom" Style="{StaticResource TextBoxTerminal}" Margin="10" KeyDown="TextBoxCommandInput_KeyDown"></TextBox>
		<TextBox x:Name="TextBoxCommandOutput" IsReadOnly="True" Style="{StaticResource TextBoxTerminal}" Margin="10,0,10,0"></TextBox>
	</DockPanel>
</UserControl>
